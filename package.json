{
  "name": "ember-frost-chart",
  "version": "4.0.0",
  "description": "Composable charts with decoupled axes / plots and some extra pizzazz",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "lint-all-the-things",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test"
  },
  "repository": "git@github.com:ciena-frost/ember-frost-chart.git",
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Steven Glanzer (https://github.com/sglanzer)",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "chai-jquery": "^2.0.0",
    "ember-cli": "2.12.3",
    "ember-cli-chai": "0.3.2",
    "ember-cli-code-coverage": "0.3.5",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-htmlbars-inline-precompile": "^0.3.6",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "0.13.2",
    "ember-cli-shims": "^1.0.2",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-code-snippet": "^1.8.0",
    "ember-d3": "0.3.2",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-elsewhere": "0.4.1",
    "ember-export-application-global": "^1.0.5",
    "ember-frost-core": "^1.14.3",
    "ember-frost-test": "^1.0.0",
    "ember-load-initializers": "^0.6.0",
    "ember-resolver": "^2.0.3",
    "ember-sinon": "0.6.0",
    "ember-source": "~2.12.0",
    "ember-spread": "1.1.4",
    "ember-test-utils": "^4.0.0",
    "ember-truth-helpers": "1.3.0",
    "loader.js": "^4.2.3",
    "sinon-chai": "^2.8.0"
  },
  "keywords": [
    "ember-addon",
    "frost"
  ],
  "dependencies": {
    "d3": "^4.7.4",
    "ember-cli-babel": "^5.1.7",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-sass": "^5.6.0",
    "ember-computed-decorators": "0.3.0",
    "ember-concurrency": "0.7.19",
    "ember-hook": "^1.4.1",
    "ember-prop-types": "^3.11.0",
    "ember-resize": "0.0.17"
  },
  "ember-addon": {
    "after": [
      "ember-computed-decorators"
    ],
    "configPath": "tests/dummy/config"
  }
}
